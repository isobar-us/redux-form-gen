{
  "name": "@isobar-us/redux-form-gen",
  "version": "0.8.1",
  "description": "A pluggable form generator for redux-form",
  "main": "./lib/index.js",
  "files": [
    "dist",
    "lib",
    "src",
    "css"
  ],
  "style": "dist/style.css",
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "check": "npm run lint && npm run test && yarn run flow",
    "lint": "eslint --ext=js,jsx src __tests__ stories webpack.config.js",
    "lint:fix": "npm run lint -- --fix",
    "build:css": "node-sass css/styles.scss dist/style.css",
    "build:lib": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:flow": "flow-copy-source -v -i '**/__tests__/**' src lib",
    "build:umd": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack src/index.js dist/redux-form-gen.js",
    "build:umd:min": "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack src/index.js dist/redux-form-gen.min.js",
    "build": "npm run build:lib && npm run build:flow && npm run build:umd && npm run build:umd:min && npm run build:css",
    "deploy-storybook": "storybook-to-ghpages",
    "test": "cross-env BABEL_ENV=commonjs jest",
    "test:cov": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "coverage:report": "cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage",
    "format": "prettier-eslint \"+(src|stories|__tests__)/**/*.+(js)\" --ignore \"**/bundle.js\" --write",
    "prepublishOnly": "npm run clean && npm run check && npm run build --kill --print",
    "storybook": "start-storybook -p 6006",
    "start": "npm run storybook",
    "build-storybook": "build-storybook",
    "precommit": "npm run lint -q",
    "prepush": "npm run check -q"
  },
  "author": "Isobar (http://github.com/isobar-us)",
  "license": "MIT",
  "keywords": [
    "redux-form",
    "redux",
    "form",
    "generator",
    "isobar",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/isobar-us/redux-form-gen"
  },
  "bugs": {
    "url": "https://github.com/isobar-us/redux-form-gen/issues"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "codacy-coverage": "^2.0.3",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.16",
    "@storybook/storybook-deployer": "^2.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.3.13",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.x",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.60.1",
    "flow-copy-source": "^1.2.1",
    "husky": "^0.14.3",
    "jest": "^22.1.1",
    "node-sass": "^4.5.0",
    "prettier": "^1.8.2",
    "prettier-eslint-cli": "^4.4.0",
    "raf": "^3.3.2",
    "react": "^16.0.0-rc.3",
    "react-codemirror": "^1.0.0",
    "react-dom": "^16.0.0-rc.3",
    "react-redux": "^5.0.5",
    "react-test-renderer": "^16.0.0-rc.3",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.3.0"
  },
  "jestConfig": "./jest.config.js",
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0-0",
    "react-dom": "^0.14.0 || ^15.0.0 || ^16.0.0-0",
    "react-redux": "^5.0.0",
    "redux-form": "^6.7.0 || ^7.0.0"
  }
}
